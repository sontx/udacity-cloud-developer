{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "strict": false,
    "skipLibCheck": true,
    "types": []
  },
  "exclude": [
    "node_modules/**/*",
  ]
}
